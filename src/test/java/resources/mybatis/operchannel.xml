<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" " http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mybatis.stu.old.OperChannelMapper">

    <sql id="testSql">

    </sql>

    <select id="selectAll" parameterType="int" resultType="com.mybatis.stu.old.OperChannel" useCache="true">
        select * from OperChannel where id = #{id}
    </select>

    <select id="selectByIdAndName" parameterType="com.mybatis.stu.old.OperChannel" resultType="com.mybatis.stu.old.OperChannel">
        select * from OperChannel
        <where>
            <!--<if test="operChannelId != null and operChannelId != ''">-->
                <!--and operChannelId = #{operChannelId}-->
            <!--</if>-->
            <if test="operChannelName != null and operChannelName != ''">
                and operChannelName = #{operChannelName}
            </if>
        </where>
    </select>

    <select id="selectNameByIdAndName" parameterType="String" resultType="String">
        select operChannelName from OperChannel where operChannelName = #{_operChannelName}
    </select>
<!--<cache></cache>-->
</mapper>